  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "dotenv -e .env.test -e .env -- jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "x": "dotenv -- cross-var docker exec -it %COMPOSE_PROJECT_NAME%_dna",
    "x:yarn": "yarn x yarn",
    "dev:start": "/bin/bash scripts/compose.sh up",
    "dev:stop": "/bin/bash scripts/compose.sh down",
    "make": "/bin/bash scripts/make.sh",
    "husky:install": "husky install"
  },
